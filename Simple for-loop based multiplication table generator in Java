 import java.util.Scanner;
   class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int num1;
        System.out.print("Enter any number:");
        num1 =input.nextInt();
        for(int i= 0;i<10;i++)
        {
         System.out.println(num1 + " X " + (i+1) +" = "+ (num1 * (i+1) ));
        }
    }
}
